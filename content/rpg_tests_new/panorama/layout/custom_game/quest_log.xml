<root>  
       
	<styles>       
		<include src="file://{resources}/styles/custom_game/quest.css" />
		<include src="file://{resources}/styles/dotastyles.css" />
	</styles>     
	         
	<scripts> 
		<include src="file://{resources}/scripts/custom_game/quest.js" />
	</scripts>       
	    
	<Panel>  
		<Panel id="QuestRoot" class="QuestRoot" hittest="false">
			<Panel id="Background" hittest="false" />
			<Panel id="Stuff" hittest="false">
				<Panel id="Title" hittest="false">   
					<Image id="QuestLog" src="file://{images}/custom_game/ItemIcon058.png"  hittest="false"/>
					<Label id="QuestTitle" text="Quest log" hittest="false"
					/> 
					<Button id="MiniPanel" onactivate="OnQuestHide()" >
						<Label id="MiniLabel" text=" --- " onclick="javascript:$.msg('HI');" />
					</Button>
				</Panel> 
				<Panel id="ContainerPanel" hittest="false">
				</Panel>   
			</Panel>
			<Panel id="Compiling"  hittest="false">   
				<Image src="file://{images}/custom_game/background.jpg" />
			</Panel>
		</Panel>
	</Panel>
	 
	 
	
</root>
 